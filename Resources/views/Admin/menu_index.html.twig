{% block root %}
    <div class="panel panel-default">
        <div class="panel-body">
            <nav>
                <ul class="nav">
                    {% for child in item.children %}
                        <li><a href="{{ child.uri }}">{{ child.label|raw }}</a></li>
                    {% endfor %}
                </ul>
            </nav>
        </div>
    </div>
{% endblock %}